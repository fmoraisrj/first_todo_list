 
<ul id="lists" class="unstyled sort lists-container"  >
  <% @lists.each do |list| %>
  <li data-id="<%= list.id %>">
    <strong><%= list.name_list  %></strong>
    <span class="actions">
      <%= link_to 'Show', list_path(list) %><span class="dot">&middot;</span>
      <%= link_to 'Edit', edit_list_path(list) %><span class="dot">&middot;</span>
      <%= link_to 'Destroy', list, :confirm => 'Are you sure?', :method => :delete %>
    </span>
  </li>
  <% end %>
</ul>


