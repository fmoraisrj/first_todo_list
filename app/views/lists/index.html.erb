 
<%- flash.each do |key, msg| -%>
<div id="<%= key %>">
  <p style="float:right;"><%= link_to_function 'X', "Effect.Fade('#{key}')" %></p>
  <p><%= msg %></p>
  <div class="clear"></div>
</div>
<%- end -%>
<ul class="unstyled sort lists-container"  >
  <% @lists.each do |list| %>
  <li>
    <strong><%= list.name_list  %></strong>
    <span class="actions">
      <%= link_to 'Show', list_path(list) %><span class="dot">&middot;</span>
      <%= link_to 'Edit', edit_list_path(list) %><span class="dot">&middot;</span>
      <%= link_to 'Destroy', list, :confirm => 'Are you sure?', :method => :delete %>
    </span>
  </li>
  <% end %>
</ul>


